{
  "name": "Bookmarks Management",
  "version": "1.0.2",
  "manifest_version": 2,
  "description": "Simple extension to manage all of your bookmarks",
  "author": "TIKAM CHAND",
  "homepage_url": "http://tcmhack.in/projects/bookmarks-management/",
  "icons": {
    "128": "icon.png",
    "16": "icon.png",
    "22": "icon.png",
    "32": "icon.png",
    "48": "icon.png"
  },
  "browser_action": {
    "default_title": "Bookmarks Management",
    "default_icon": "icon.png",
    "default_popup": "index.html"
  },
  "permissions": ["bookmarks"]
}
